package model.entities;

public class Circulo extends Formas{

	private Double raio;

	public Circulo(Double raio) {
		super();
		this.raio = raio;
	}

	@Override
	public Double area() {
		return Math.PI * raio * raio;
	}
	
	public Double getRaio() {
		return raio;
	}

	public void setRaio(Double raio) {
		this.raio = raio;
	}




}
