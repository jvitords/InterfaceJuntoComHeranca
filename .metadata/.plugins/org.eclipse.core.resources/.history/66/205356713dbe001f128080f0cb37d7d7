package model.entities;

import model.enums.Color;

public abstract class FormasEmAbstract implements Formas{ 
	// implementa o "contrato" da interface "Fomras", mas como essa class é abstrata, ela não precisa usar os métodos desse contrato
	
	private Color cor;

	public FormasEmAbstract(Color cor) {
		this.cor = cor;
	}

	public FormasEmAbstract() {
	}
	public Color getCor() {
		return cor;
	}

	public void setCor(Color cor) {
		this.cor = cor;
	}
}
